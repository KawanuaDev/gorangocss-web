body{font-family:fira sans,sans-serif}pre,code,kbd,samp{font-family:fira mono,monospace!important;font-size:.8rem!important}pre,code{display:block;line-height:1.8}kbd{background-color:#f5f5f5;border-radius:.3rem;display:inline-block;padding:.2rem;vertical-align:baseline}.class,.preview{background-image:url(data:image/svg+xml;base64,);border-color:#f5f5f5!important}.preview{justify-content:space-evenly}.navbar--fixed{width:100vw;position:fixed}.navbart{padding:.75rem}.navbart__container{display:flex;justify-content:space-between;align-items:center}.navbart__burger{display:none}.navbart__logo{padding:8px}.navbart__logo a{color:#000;text-transform:uppercase;text-decoration:none;font-weight:700;font-size:18px;letter-spacing:0}.navbart ul{float:right}.navbart ul li{display:inline-block}.navbart ul li:not(:first-child){margin-left:48px}.navbart ul li:last-child{margin-right:24px}.navbart ul li a{outline:none;text-transform:uppercase;text-decoration:none;font-size:14px;font-weight:600;letter-spacing:1.2}@media screen and (max-width:864px){.navbart__logo{padding:0}.navbart__container{flex-direction:column}.navbart__container>div{margin-top:.25rem;margin-bottom:.25rem}.navbart__wrapper{position:fixed;top:0;left:0;width:100%;height:100%;z-index:-1;background:#fff;opacity:0;transition:all .2s ease}.navbart__wrapper ul{position:absolute;top:22rem;transform:translateY(-50%);width:100%}.navbart__wrapper ul li{display:block;float:none;width:100%;text-align:right;margin-bottom:10px}.navbart__wrapper ul li:nth-child(1) a{transition-delay:.2s}.navbart__wrapper ul li:nth-child(2) a{transition-delay:.3s}.navbart__wrapper ul li:nth-child(3) a{transition-delay:.4s}.navbart__wrapper ul li:nth-child(4) a{transition-delay:.5s}.navbart__wrapper ul li:not(:first-child){margin-left:0}.navbart__wrapper ul li a{padding:10px 24px;opacity:0;color:#000;font-size:14px;font-weight:600;letter-spacing:1.2;transform:translateX(-20px);transition:all .2s ease}.navbart__burger{position:fixed;right:1.2rem;display:block!important;width:48px;height:48px;cursor:pointer;z-index:9999;border-radius:50%}.navbart__burger span{display:block;width:20px;height:2px;background:#000;border-radius:2px;margin-left:14px}.navbart__burger span:nth-child(1){margin-top:16px}.navbart__burger span:nth-child(2){margin-top:4px;opacity:1}.navbart__burger span:nth-child(3){margin-top:4px}}#navt:checked+.navbart__burger{transform:rotate(45deg)}#navt:checked+.navbart__burger span{background:#000;transition:transform .2s ease}#navt:checked+.navbart__burger span:nth-child(1){transform:translateY(6px)rotate(180deg)}#navt:checked+.navbart__burger span:nth-child(2){opacity:0}#navt:checked+.navbart__burger span:nth-child(3){transform:translateY(-6px)rotate(90deg)}#navt:checked~.navbart__wrapper{z-index:9990;opacity:1}#navt:checked~.navbart__wrapper ul li a{opacity:1;transform:translateX(0)}#navt:checked~.navbart__wrapper .dropdown__menu{display:block;position:initial}#navt:checked~.navbart__wrapper .dropdown__menu .dropdown__content{box-shadow:none;border-radius:0;display:flex;flex-wrap:wrap}